<Window x:Class="Tracing.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Tracing" 
        Height="350" Width="525">
    <DockPanel>
        <WrapPanel>
            <Label>Number</Label>
            <TextBox Text="{Binding Number}" Width="80"/>
            <Button Command="{Binding CallCommand}">Call</Button>
            <TextBox Text="{Binding CallResult}" />
        </WrapPanel>
    </DockPanel>
</Window>
